import java.util.Scanner;
public class Major {
	public static void main (String[]args) {
		Scanner input = new Scanner (System.in);
		
		System.out.print("Enter two characters: ");
		
		String major = input.next();
		input.close();
		
		if (major.equals("M1"))
			System.out.print("Mathematics Freshman");	
		else if (major.equals("M2"))
		System.out.print("Mathematics Sophmore");
		else if (major.equals("M3"))
			System.out.print("Mathematics Junior");
		else if (major.equals("M4"))
			System.out.print("Mathematics Senior");
		else if (major.equals("C1"))
			System.out.print("Computer Science Freshman");
		else if (major.equals("C2"))
			System.out.print("Computer Science Sophmore");
		else if (major.equals("C3"))
			System.out.print("Computer Science Junior");
		else if (major.equals("C4"))
			System.out.print("Computer Science Senior");
		else if (major.equals("I1"))
			System.out.print("Information Technology Freshman");
		else if (major.equals("I2"))
			System.out.print("Information Technology Sophmore");
		else if (major.equals("I3"))
			System.out.print("Information Technology Junior");
		else if (major.equals("I4"))
			System.out.print("Information Technology Senior");
		else
			System.out.println("Invalid input");
		
		
		
		
	}
}
